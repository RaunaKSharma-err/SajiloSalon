{
  "expo": {
    "name": "Salon Manager App",
    "slug": "salon-manager-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/salonLogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.VIBRATE"]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [["expo-router"]],
    "experiments": {
      "typedRoutes": true
    }
  }
}
