{
  "expo": {
    "name": "Salon Manager App",
    "slug": "salon-manager-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/salonLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.VIBRATE"],
      "package": "com.sajiloSalon"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [["expo-router"], "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7c8c7a91-97df-4b09-9561-c89f40589ea5"
      }
    }
  }
}
